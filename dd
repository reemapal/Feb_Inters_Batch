select atb_desc,sub_desc,tc_desc,intern_name,remark from attribute a inner join sub_attribute s on a.atb_id=s.atb_id inner join test_cases t on s.sub_id=t.sub_id inner join intern_score p on t.test_id=p.test_id inner join interns i on p.intern_id=i.intern_id;
